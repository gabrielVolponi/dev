{
  "dependencies": {
    "@tailwindcss/cli": "^4.0.17",
    "tailwindcss": "^4.0.17"
  },
  "devDependencies": {
    "cypress": "^14.2.1"
  },
  "scripts": {
    "build:css": "tailwindcss -i ./src/style.css -o ./src/output.css --watch",
    "build": "npm run build:css",
    "cypress:run": "npm run build:css && cypress run",
    "cypress:open": "npm run build:css && cypress open"
  }
}
